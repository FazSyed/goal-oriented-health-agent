<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#"
     xml:base="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:HealthAgent="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent">
        <rdfs:comment>Phase 1 of Goal-Oriented Agents for Real-time Decision-Making in Elderly Care. This phase focuses on a Single Goal - maintaining the patients hydration above threshold value ie preventing risk of dehydration.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasRiskStatus -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasRiskStatus">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#RiskStatus"/>
        <rdfs:comment>Patient may have a certain Risk Status depending on their hydration and threshold value</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasThreshold -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasThreshold">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Threshold"/>
        <rdfs:comment>Patient has a Threshold value (i.e.) a value below which they are considered to be dehydrated</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#triggersAction -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#triggersAction">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#RiskStatus"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#AlertAction"/>
        <rdfs:comment>If RiskStatus detected, it triggers an action (Alerts a text)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#TBWLossPercent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#TBWLossPercent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#thresholdValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#thresholdValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Threshold"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#AlertAction -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#AlertAction">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendEmergencySignal"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendHydrationReminder"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendIVRequiredAlert"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Alert an Action when a Risk Status is triggered</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasRiskStatus"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#RiskStatus"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>A Patient with a certain required hydration level, where the hydration value is less than or equal to 1500 ml. They have a Risk (status) of a specific Dehydration level.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#MildlyDehydratedPatient -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#MildlyDehydratedPatient">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasRiskStatus"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Mild"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient"/>
        <rdfs:comment>A Dehydrated Patient with a dehydration Risk Status of Mild</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#ModeratelyDehydratedPatient -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#ModeratelyDehydratedPatient">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasRiskStatus"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Moderate"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient"/>
        <rdfs:comment>A Dehydrated Patient with a dehydration Risk Status of Moderate</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient">
        <rdfs:comment>Elderly person being monitored</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#RiskStatus -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#RiskStatus">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Mild"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Moderate"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Severe"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Risk Status for each level of Dehydration (Enumerated class)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SeverelyDehydratedPatient -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SeverelyDehydratedPatient">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasRiskStatus"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Severe"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#DehydratedPatient"/>
        <rdfs:comment>A Dehydrated Patient with a dehydration Risk Status of Severe</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Threshold -->

    <owl:Class rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Threshold">
        <rdfs:comment>A value, below which, the patient is at risk of dehydration</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Mild -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Mild"/>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Moderate -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Moderate"/>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient1">
        <rdf:type rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient"/>
        <TBWLossPercent rdf:datatype="http://www.w3.org/2001/XMLSchema#float">4.6</TBWLossPercent>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendEmergencySignal -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendEmergencySignal">
        <rdf:type rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#AlertAction"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendHydrationReminder -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendHydrationReminder"/>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendIVRequiredAlert -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendIVRequiredAlert"/>
    


    <!-- http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Severe -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Severe">
        <rdf:type rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#RiskStatus"/>
    </owl:NamedIndividual>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#MildlyDehydratedPatient"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#ModeratelyDehydratedPatient"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SeverelyDehydratedPatient"/>
        </owl:members>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Rules
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#p">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#v">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment></rdfs:comment>
        <rdfs:label>SevereDehydrationRule</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient"/>
                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#p"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#TBWLossPercent"/>
                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#p"/>
                                <swrl:argument2 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#v"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#BuiltinAtom"/>
                                        <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                                        <swrl:arguments>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#v"/>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">10.0</rdf:first>
                                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </swrl:arguments>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#hasRiskStatus"/>
                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#p"/>
                        <swrl:argument2 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Severe"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#triggersAction"/>
                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#p"/>
                                <swrl:argument2 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#SendEmergencySignal"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment></rdfs:comment>
        <rdfs:label>MildDehydrationRule</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#Patient"/>
                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#p"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#TBWLossPercent"/>
                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#p"/>
                                <swrl:argument2 rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#v"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#BuiltinAtom"/>
                                        <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                                        <swrl:arguments>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first rdf:resource="http://www.semanticweb.org/fazilasyed/ontologies/2025/6/HealthAgent#v"/>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">2.0</rdf:first>
                                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </swrl:arguments>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
